{{% BLOCKS }}
{{< panel}}
	{{$content}}
 		<div class="request-holder">
 			<div class="request">
 				<header>
					<h1>
						Profile
					</h1>
				</header>
				{{#user}}
				 		<table>
				 			<tr>
								<td><b>User</b></td>
								<td>{{usuario}}</td>
							</tr>
							<tr>
								<td><b>Name</b></td>
								<td>{{nombre}}</td>
							</tr>
							<tr>
								<td><b>Last Name</b></td>
								<td>{{papellido}}</td>
							</tr>
							<tr>
								<td><b>Mother's Last Name</b></td>
								<td>{{mapellido}}</td>
							</tr>
							<tr>
		            			<td><b>Phone:</b></td>
		            			<td>{{telefono}}</td>
				            </tr>
				            <tr>
		            			<td><b>Iata:</b></td>
		            			<td>{{iata}}</td>
				            </tr>
				            <tr>
		            			<td><b>Member:</b></td>
		            			<td>{{miembros}}</td>
				            </tr>
				            <tr>
		            			<td><b>Years in bussiness:</b></td>
		            			<td>{{years}}</td>
				            </tr>
				            <tr>
		            			<td><b>Active since:</b></td>
		            			<td>{{create_at.getTimeStamp | date.format}}</td>
				            </tr>
						</table>
						<br>
						<center>
				 			<button type="button" onclick="window.location.href='/bali/en/editProfile'">EDIT</button>
				 		</center>
				{{/user}}
 			</div>
 		</div>
	{{/content}}
	{{$ scripts}}
	{{/ scripts}}
{{/ panel}}